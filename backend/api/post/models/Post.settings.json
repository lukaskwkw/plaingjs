{
  "connection": "default",
  "collectionName": "posts",
  "info": {
    "name": "post",
    "description": "A blog post"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "cover": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    }
  }
}